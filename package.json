{
  "name": "roomfindr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --run src/test/*.test.ts",
    "test:components": "vitest --run src/test/*.test.tsx",
    "test:e2e": "vitest --run src/test/e2e-integration.test.ts",
    "test:performance": "vitest --run src/test/performance.test.ts",
    "test:accessibility": "vitest --run src/test/accessibility.test.tsx",
    "test:comprehensive": "tsx src/test/test-runner.ts",
    "test:coverage": "vitest --run --coverage",
    "db:setup": "node scripts/setup-database.js",
    "db:test": "node scripts/setup-database.js --test",
    "db:check": "node scripts/setup-database.js --check",
    "db:migrate": "node scripts/apply-migrations.js",
    "db:seed": "node scripts/load-seed-data.js",
    "storage:setup": "node scripts/setup-storage.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.87.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-dropzone": "^14.3.8",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.27",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-check": "^4.4.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
}
